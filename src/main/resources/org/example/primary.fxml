<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<HBox maxHeight="800.0" maxWidth="1000.0" minHeight="800.0" minWidth="1000.0" prefHeight="800.0" prefWidth="1000.0" style="-fx-background-color: #11151c;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.PrimaryController">
   <children>
      <Pane fx:id="graphEditor" maxHeight="760.0" maxWidth="560.0" minHeight="760.0" minWidth="560.0" prefHeight="760.0" prefWidth="560.0" style="-fx-background-color: #364156;">
         <HBox.margin>
            <Insets bottom="20.0" left="20.0" top="20.0" />
         </HBox.margin>
      </Pane>
      <VBox fx:id="menu" maxHeight="760.0" maxWidth="390.0" minHeight="760.0" minWidth="390.0" prefHeight="760.0" prefWidth="390.0" style="-fx-background-color: #212d40;">
         <HBox.margin>
            <Insets bottom="20.0" left="10.0" top="20.0" />
         </HBox.margin>
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #d66853;">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <children>
                  <Label alignment="TOP_LEFT" contentDisplay="CENTER" text="Editor ">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Button fx:id="clearGraphEditor" mnemonicParsing="false" text="Clear all">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                  </Button>
                  <RadioButton fx:id="startNode" mnemonicParsing="false" text="Choose start node">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="endNode" mnemonicParsing="false" text="Choose end node">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="addVertex" mnemonicParsing="false" text="Add vertex">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="removeVertex" mnemonicParsing="false" text="Remove vertex">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="addEdge" mnemonicParsing="false" text="Add edge">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="removeEdge" mnemonicParsing="false" text="Remove Edge">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                  </RadioButton>
                  <RadioButton mnemonicParsing="false" text="Directed edges">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                  </RadioButton>
                  <Slider fx:id="vertexCount">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" />
                     </VBox.margin>
                  </Slider>
                  <Slider>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" />
                     </VBox.margin>
                  </Slider>
                  <HBox>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                     <children>
                        <Button fx:id="generate" mnemonicParsing="false" text="Generate">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="save" mnemonicParsing="false" text="Save">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="load" mnemonicParsing="false" text="Load" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #7d4e57;">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </VBox.margin>
               <children>
                  <Label text="Simulation">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <RadioButton fx:id="dijkstra" mnemonicParsing="false" text="Dijkstra">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </VBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="aStar" mnemonicParsing="false" text="A*">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                  </RadioButton>
                  <RadioButton fx:id="antOptimization" mnemonicParsing="false" text="Ant Optimization">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" />
                     </VBox.margin>
                  </RadioButton>
                  <Slider fx:id="simSpeed">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" />
                     </VBox.margin>
                  </Slider>
               </children>
            </VBox>
            <Button fx:id="start" maxWidth="195.0" mnemonicParsing="false" prefHeight="100.0" text="Start" textAlignment="CENTER">
               <VBox.margin>
                  <Insets left="185.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Line endX="100.0" startX="-100.0" />
   </children>
</HBox>
