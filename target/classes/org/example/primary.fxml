<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<SplitPane fx:id="splitPane" dividerPositions="0.6353383458646616" minHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.PrimaryController">
   <items>
      <VBox>
         <children>
            <MenuBar style="-fx-background-color: #d8dbe2;">
               <menus>
                  <Menu mnemonicParsing="false" text="File">
                    <items>
                      <MenuItem fx:id="neww" mnemonicParsing="false" text="New" />
                        <MenuItem fx:id="load" mnemonicParsing="false" text="Load" />
                        <MenuItem fx:id="save" mnemonicParsing="false" text="Save" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Help">
                    <items>
                      <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                  </Menu>
               </menus>
            </MenuBar>
            <Pane fx:id="graphEditor" style="-fx-background-color: #373f51;" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
      <TabPane style="-fx-background-color: #f2d492;">
         <tabs>
            <Tab closable="false" style="-fx-background-color: #d8dbe2;" text="Editor">
              <content>
                  <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER">
                     <content>
                        <VBox style="-fx-background-color: #598392;">
                           <children>
                              <Label alignment="TOP_LEFT" contentDisplay="CENTER" text="Editor ">
                                 <font>
                                    <Font name="System Bold" size="20.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                              </Label>
                              <RadioButton fx:id="startNode" mnemonicParsing="false" text="Choose start node">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <RadioButton fx:id="endNode" mnemonicParsing="false" text="Choose end node">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <RadioButton fx:id="addVertex" mnemonicParsing="false" text="Add vertex">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <RadioButton fx:id="removeVertex" mnemonicParsing="false" text="Remove vertex">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <RadioButton fx:id="edgeLabels" mnemonicParsing="false" text="Show edge labels">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <VBox alignment="CENTER" style="-fx-border-color: #468189; -fx-border-radius: 10 10 10 10;">
                                 <children>
                                    <Label text="Vertex radius">
                                       <VBox.margin>
                                          <Insets left="10.0" right="10.0" />
                                       </VBox.margin>
                                    </Label>
                                    <Slider fx:id="vertexRadius">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                       <VBox.margin>
                                          <Insets bottom="10.0" left="10.0" right="10.0" />
                                       </VBox.margin>
                                    </Slider>
                                 </children>
                                 <opaqueInsets>
                                    <Insets left="10.0" right="10.0" />
                                 </opaqueInsets>
                              </VBox>
                              <VBox alignment="CENTER" style="-fx-border-color: #468189; -fx-border-radius: 10 10 10 10;">
                                 <children>
                                    <Label text="Edge width">
                                       <VBox.margin>
                                          <Insets left="10.0" right="10.0" />
                                       </VBox.margin>
                                    </Label>
                                    <Slider fx:id="edgeWidth">
                                       <opaqueInsets>
                                          <Insets />
                                       </opaqueInsets>
                                       <VBox.margin>
                                          <Insets bottom="10.0" left="10.0" right="10.0" />
                                       </VBox.margin>
                                    </Slider>
                                 </children>
                                 <opaqueInsets>
                                    <Insets left="10.0" right="10.0" />
                                 </opaqueInsets>
                              </VBox>
                              <HBox alignment="BOTTOM_RIGHT">
                                 <VBox.margin>
                                    <Insets />
                                 </VBox.margin>
                              </HBox>
                              <Button fx:id="clearGraphEditor" mnemonicParsing="false" text="Clear all">
                                 <VBox.margin>
                                    <Insets left="10.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </content>
                  </ScrollPane>
              </content>
            </Tab>
            <Tab closable="false" style="-fx-background-color: #d8dbe2;" text="Generator">
              <content>
                  <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER">
                     <content>
                        <VBox style="-fx-background-color: #9dbebb;">
                           <children>
                              <Label text="Generator">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                                 <font>
                                    <Font name="System Bold" size="20.0" />
                                 </font>
                              </Label>
                              <RadioButton mnemonicParsing="false" text="Directed graph">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <VBox alignment="CENTER">
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                                 <children>
                                    <VBox alignment="CENTER" style="-fx-border-color: #468189; -fx-border-radius: 10 10 10 10;">
                                       <VBox.margin>
                                          <Insets bottom="10.0" left="10.0" right="10.0" />
                                       </VBox.margin>
                                       <HBox.margin>
                                          <Insets bottom="20.0" left="20.0" right="10.0" />
                                       </HBox.margin>
                                       <children>
                                          <Label text="Number of vertices">
                                             <VBox.margin>
                                                <Insets left="10.0" right="10.0" />
                                             </VBox.margin>
                                          </Label>
                                          <Slider fx:id="vertexCount">
                                             <opaqueInsets>
                                                <Insets />
                                             </opaqueInsets>
                                             <VBox.margin>
                                                <Insets bottom="10.0" left="10.0" right="10.0" />
                                             </VBox.margin>
                                          </Slider>
                                       </children>
                                    </VBox>
                                    <HBox alignment="CENTER">
                                       <children>
                                          <Button fx:id="generate" mnemonicParsing="false" text="Generated" />
                                       </children>
                                       <VBox.margin>
                                          <Insets bottom="10.0" top="10.0" />
                                       </VBox.margin>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                     </content>
                  </ScrollPane>
              </content>
            </Tab>
            <Tab closable="false" style="-fx-background-color: #d8dbe2;" text="Simulation">
               <content>
                  <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER">
                     <content>
                        <VBox style="-fx-background-color: #58a4b0;">
                           <children>
                              <Label text="Simulation">
                                 <font>
                                    <Font name="System Bold" size="20.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" top="10.0" />
                                 </VBox.margin>
                              </Label>
                              <RadioButton fx:id="dijkstra" mnemonicParsing="false" text="Dijkstra">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" top="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <RadioButton fx:id="aStar" mnemonicParsing="false" text="A*">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <RadioButton fx:id="bellmanFord" mnemonicParsing="false" text="Bellman-Ford">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <RadioButton fx:id="genetic" mnemonicParsing="false" text="Genetic">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <VBox spacing="10.0" style="-fx-border-color: #264653; -fx-border-radius: 10 10 10 10;">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" />
                                 </VBox.margin>
                                 <children>
                                    <RadioButton fx:id="antOptimization" mnemonicParsing="false" text="Ant Optimization">
                                       <VBox.margin>
                                          <Insets bottom="10.0" />
                                       </VBox.margin>
                                    </RadioButton>
                                    <VBox>
                                       <children>
                                          <Label text="Number of ants" />
                                          <Slider fx:id="numberOfAnts" />
                                       </children>
                                    </VBox>
                                    <VBox>
                                       <children>
                                          <Label text="Pheromone per ant" />
                                          <Slider fx:id="pheromonePerAnt" />
                                       </children>
                                    </VBox>
                                    <VBox>
                                       <children>
                                          <Label text="Alpha" />
                                          <Slider fx:id="alpha" />
                                       </children>
                                    </VBox>
                                    <VBox>
                                       <children>
                                          <Label text="Beta" />
                                          <Slider fx:id="beta" />
                                       </children>
                                    </VBox>
                                    <VBox>
                                       <children>
                                          <Label text="Evaporation rate" />
                                          <Slider fx:id="evaporationRate" />
                                       </children>
                                    </VBox>
                                    <VBox>
                                       <children>
                                          <Label text="Number of iterations" />
                                          <Slider fx:id="numberOfIterations" />
                                       </children>
                                    </VBox>
                                 </children>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </VBox>
                              <VBox alignment="CENTER" style="-fx-border-color: #264653; -fx-border-radius: 10 10 10 10;">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" />
                                 </VBox.margin>
                                 <children>
                                    <Label text="Simulation speed">
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </Label>
                                    <Slider fx:id="simulationSpeed">
                                       <VBox.margin>
                                          <Insets bottom="10.0" left="10.0" right="10.0" />
                                       </VBox.margin>
                                    </Slider>
                                 </children>
                              </VBox>
                              <RadioButton fx:id="showWeights" mnemonicParsing="false" selected="true" text="Show weights">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <RadioButton fx:id="generateReport" mnemonicParsing="false" text="Generate raport">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <RadioButton fx:id="showAnimation" mnemonicParsing="false" text="Show animation">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" />
                                 </VBox.margin>
                              </RadioButton>
                              <Button fx:id="start" mnemonicParsing="false" prefHeight="50.0" prefWidth="68.0" text="Start" textAlignment="CENTER">
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                        </VBox>
                     </content>
                  </ScrollPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </items>
</SplitPane>
